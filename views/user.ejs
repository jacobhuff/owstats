<% include partials/header.ejs %>

<div class="user-container">
    <div class="information-div">
        <div class="information">
            Stats shown are per 10 minutes of competitive gameplay.
            You must play at least 10 games for stats to be displayed.
            Try out our compare feature!
        </div>
    </div>

    <div class="user-row">
        <div class="user-icon-div">
            <img class="user-icon" src=<%= userData.profile_image %>>
        </div>
        <div class="user-contents-div">
            <div class="user-name"><%= userData.username %></div>
            <div class="user-data">
                <div class="general-user-data">
                    Season: <%= userData.season %> 
                    <span class="divider">
                        |
                    </span> 
                    Rank: <%= userData.user_rank %> 
                    <span class="divider">
                        |
                    </span> 
                    Level: <%= userData.level %>
                </div>
            </div>
        </div>
        <div class="refresh-icon-div">
            <form method="post" action="/update-stats/<%= userData.username %>/platform=<%= platform %>">
                <button type="submit" class="fas fa-sync-alt fa-3x user-refresh"></button>
            </form>
        </div>
    </div>

    <div class="buttons-div">
        <form method="post" action="/user/<%= userData.username %>/compare/platform=<%= platform %>">
            <button type="submit" class="compare-button">Compare</button>
        </form>
    </div>

